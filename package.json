{
  "name": "io-cep",
  "version": "2.1.0",
  "description": "Busca por informações de uma localidade através do endereço ou CEP utilizando os Correios",
  "keywords": [
    "cep",
    "lib",
    "search",
    "zipcode",
    "brasil"
  ],
  "bug": {
    "url": "https://github.com/lagden/io-cep/issues",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "http://lagden.in"
  },
  "main": "index.js",
  "directories": {
    "lib": "./lib"
  },
  "repository": "lagden/io-cep",
  "scripts": {
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "pretest": "xo",
    "test": "istanbul cover _mocha --report lcovonly -- -R spec -t 5000",
    "start": "watch xo"
  },
  "dependencies": {
    "cheerio": "^0.19.0",
    "got": "^5.0.0",
    "iconv-lite": "^0.4.11",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "watch": "^0.16.0",
    "xo": "^0.10.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "xo": {
    "envs": [
      "es6",
      "node"
    ],
    "esnext": true,
    "rules": {
      "arrow-parens": [
        2,
        "as-needed"
      ],
      "prefer-reflect": [
        2,
        {
          "exceptions": [
            "call"
          ]
        }
      ],
      "new-cap": [
        2,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ],
      "lines-around-comment": 0
    }
  }
}
