{
  "name": "io-cep",
  "version": "0.5.0",
  "description": "Search address using zip code through Correios",
  "license": "MIT",
  "repository": "lagden/io-cep",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "lagden.in"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "index.js",
  "directories": {
    "lib": "./lib"
  },
  "keywords": [
    "cep",
    "lib",
    "search",
    "zipcode",
    "brasil"
  ],
  "scripts": {
    "pretest": "xo",
    "test": "nyc ava",
    "start": "watch xo"
  },
  "xo": {
    "envs": [
      "es6",
      "node"
    ],
    "esnext": true,
    "rules": {
      "arrow-parens": [
        2,
        "as-needed"
      ],
      "prefer-reflect": [
        2,
        {
          "exceptions": [
            "call"
          ]
        }
      ],
      "new-cap": [
        2,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ]
    }
  },
  "dependencies": {
    "cheerio": "^0.19.0",
    "got": "^5.0.0",
    "iconv-lite": "^0.4.11"
  },
  "devDependencies": {
    "ava": "^0.2.0",
    "nyc": "^3.2.2",
    "watch": "^0.16.0",
    "xo": "^0.9.0"
  }
}
